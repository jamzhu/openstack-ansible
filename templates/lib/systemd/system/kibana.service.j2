[Unit]
Description=Kibana4 ElasticSearch GUI
Requires=network.target
After=network.target

[Service]
Type=simple
User=nobody
Group=nobody
ExecStart=/opt/kibana-4.1.1-linux-x64/bin/kibana
SuccessExitStatus=143
Restart=always
StandardOutput=null
StandardError=journal

[Install]
WantedBy=multi-user.target
